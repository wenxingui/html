    <script>

        $(function () {
            // 从div对象找出包含p标签的div
            var div01 = $('div').has('p')
            console.log(div01);
            var div02 = $('div').has('.p1')
            console.log(div02);


            // 所有的div中排除 .div03的
            var divo1 = $('div').not('.div03')
            console.log(divo1);

             // 所有的div中排除 .div03和.div04的div
            var divo2 = $('div').not('.div03,.div04')
            console.log(divo2);


        })

    </script>

</head>

